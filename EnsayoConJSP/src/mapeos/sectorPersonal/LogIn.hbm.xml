<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.sectorPersonal.LogIn" table="login">
	
		<!-- 1.CAMPOS CLAVE -->
			<!-- Primaria -->
				<id column="idLogIn" name="idLogIn">
					<generator class="identity"/>
				</id>
			<!--  -->
		<!--  -->
	
		<!-- 2.ATRIBUTOS DESCRIPTIVOS -->
			<property column="usuario" name="usuario" type="string"/>
			<property column="contrasenia" name="contrasenia" type="string"/>
			<property column="enLinea" name="enLinea" type="boolean"/>
		<!--  -->
		
	</class>
</hibernate-mapping>